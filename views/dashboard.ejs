    <script>
      $('#profile_form').on('submit', function(e) {
        e.preventDefault();

        $.ajax({
            url: '/add-member',
            type: 'POST',
            data: $(this).serialize(),
            success: function(response) {
              console.log('response', response)
              Toastify({
                text: "Member Added Successfully",
                duration: 3000,
                style: {
                    background: "linear-gradient(to right, #00b09b, #96c93d)",
                  }
                }).showToast();
                document.getElementById('clickthis').click()
                var newTableContent = $(response).find('#Add-members-table').html();
         
                $('#Add-members-table').html(newTableContent);
            },
            error: function(xhr, status, error) {
              Toastify({
                text: xhr.responseText ,
                duration: 3000,
                style: {
                    background: "linear-gradient(to right, #FF0000, #FF7878)",
                  }
                }).showToast();
                document.getElementById('clickthis').click()
            }
        });
        document.getElementById("profile_form").reset();
    });
    function dltprofle(id){

        $.ajax({
            url: '/delete-member',
            type: 'POST',
            data:{id: id},
            success: function(response) {
              console.log('response', response)
              Toastify({
                text: "Member Deleted Successfully",
                duration: 3000,
                style: {
                    background: "linear-gradient(to right, #00b09b, #96c93d)",
                  }
                }).showToast();
                var newTableContent = $(response).find('#Add-members-table').html();
                $('#Add-members-table').html(newTableContent);
            },
            error: function(xhr, status, error) {
              Toastify({
                text: 'Something went wrong',
                duration: 3000,
                style: {
                    background: "linear-gradient(to right, #FF0000, #FF7878)",
                  }
                }).showToast();
              console.log('error response', error)
            }
        });
    }
    function updatemember(id){
      $('#update_profile_form-'+id).on('submit', function(e) {
        e.preventDefault();

        $.ajax({
            url: '/update-member',
            type: 'POST',
            data: $(this).serialize(),
            success: function(response) {
              console.log('response', response)
              Toastify({
                text: "Member updated Successfully",
                duration: 3000,
                style: {
                    background: "linear-gradient(to right, #00b09b, #96c93d)",
                  }
                }).showToast();
                document.getElementById('clickthis').click()
                var newTableContent = $(response).find('#Add-members-table').html();
         
                $('#Add-members-table').html(newTableContent);
            },
            error: function(xhr, status, error) {
              Toastify({
                text: xhr.responseText ,
                duration: 3000,
                style: {
                    background: "linear-gradient(to right, #FF0000, #FF7878)",
                  }
                }).showToast();
                document.getElementById('clickthis').click()
            }
        });
        document.getElementById('update_profile_form-'+id).reset();
    });
    }
    </script>

